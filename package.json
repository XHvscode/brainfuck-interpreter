{
    "name": "xh",
    "displayName": "xh",
    "description": "a xh language interpreter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:extension.runXH",
        "onLanguage:xh"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.runXH",
                "title": "runXH"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.runXH"
                }
            ]
        },
        "languages": [
            {
                "id": "xh",
                "aliases": [
                    "xh",
                    "XH"
                ],
                "extensions": [
                    ".xh"
                ]
            }
        ],
        "snippets": [
            {
                "language": "xh",
                "path": "./xhsnippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "tslint": "^5.20.0",
        "vscode-test": "^1.2.2",
        "vscode-debugadapter": "^1.37.1"
    }
}